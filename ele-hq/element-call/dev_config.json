{
  "default_server_config": {  // 默认服务器配置
    "m.homeserver": {
      "base_url": "https://matrix.org"  // 默认的Matrix homeserver URL
    },
    "m.identity_server": {
      "base_url": "https://vector.im"  // 默认的身份服务器 URL
    }
  },
  "disable_custom_urls": false,  // 是否禁用自定义服务器 URL 的输入
  "disable_guests": false,  // 是否禁用访客模式
  "element_call": {  // Element Call 视频通话配置
    "url": "https://call.element.io",
    "use_exclusively": false,
    "participant_limit": 100,
    "brand": "Element Call",
    "guest_spa_url": "https://call.element.io/guest"
  },
  "enable_presence_by_hs_url": {  // 按 homeserver URL 启用或禁用在线状态
    "https://matrix.org": false
  },
  "features": {  // 启用或禁用特定功能
    "feature_pinning": true,  // 启用消息固定功能
    "feature_custom_status": true,  // 启用自定义状态
    "feature_polls": true,  // 启用投票功能
    "feature_threads": true  // 启用线程功能
  },
  "integrations_ui_url": "https://scalar.vector.im/",  // 集成 UI 的 URL
  "integrations_rest_url": "https://scalar.vector.im/api",  // 集成 REST API 的 URL
  "integrations_widgets_urls": [  // 集成小部件的 URL
    "https://scalar.vector.im/_matrix/integrations/v1"
  ],
  "jitsi": {  // Jitsi 视频会议配置
    "preferred_domain": null  // 不使用 Jitsi 作为首选视频会议解决方案
  },
  "piwik": {  // Piwik（现Matomo）统计分析配置
    "url": "https://piwik.riot.im/",
    "whitelistedHSUrls": ["https://matrix.org"],
    "whitelistedISUrls": ["https://vector.im"],
    "siteId": 1
  },
  "room_directory": {  // 房间目录配置
    "servers": ["matrix.org"]
  },
  "setting_defaults": {  // 默认设置
    "MessageComposerInput.showStickersButton": false,
    "MessageComposerInput.showPollsButton": false
  },
  "show_labs_settings": true,  // 是否显示实验室设置
  "sso_immediate_redirect": false,  // 是否立即重定向到 SSO 登录
  "sso_redirect_options": null,  // SSO 重定向选项
  "voip": {  // VoIP 配置
    "obey_asserted_identity": false
  },
  "widget_build_url": "https://scalar.vector.im/api/widget/build",  // 构建小部件的 URL
  "update_base_url": "https://github.com/vector-im/element-web",  // 更新检查的 URL
  "web_base_url": "https://app.element.io",  // Element Web 的基础 URL
  "bug_report_endpoint_url": "https://element.io/bugreports/submit",  // 错误报告提交的 URL
  "default_country_code": "US",  // 默认国家代码
  "default_federate": true,  // 默认启用联邦
  "default_theme": "light",  // 默认主题
  "enable_registration": true,  // 是否启用注册
  "map_style_url": "https://api.maptiler.com/maps/streets/style.json?key=<your_maptiler_key>",  // 地图样式 URL
  "welcomeUserId": "@riot-bot:matrix.org",  // 欢迎用户的用户ID
  "report_event": {  // 事件报告配置
    "admin_message_md": "Please be sure to review our [terms of service](https://example.org/terms) before reporting a message."
  },
  "terms_and_conditions_links": [  // 条款和条件链接
    {
      "text": "Code of conduct",
      "url": "https://example.org/code-of-conduct"
    }
  ],
  "posthog": {  // Posthog 分析配置
    "project_api_key": "<your_posthog_key>",
    "api_host": "https://app.posthog.com"
  }
}
